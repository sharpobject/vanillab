<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">

<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

	<bullet label="spinz">
		<action>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>20</term>
			</changeDirection>
			<wait>20</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>25</term>
			</changeDirection>
			<wait>25</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>30</term>
			</changeDirection>
			<wait>30</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>35</term>
			</changeDirection>
			<wait>35</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>40</term>
			</changeDirection>
			<wait>40</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>45</term>
			</changeDirection>
			<wait>45</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>50</term>
			</changeDirection>
			<wait>50</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>55</term>
			</changeDirection>
			<wait>55</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>60</term>
			</changeDirection>
			<wait>60</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>65</term>
			</changeDirection>
			<wait>65</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>70</term>
			</changeDirection>
			<wait>70</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>75</term>
			</changeDirection>
			<wait>75</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>80</term>
			</changeDirection>
			<wait>80</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>85</term>
			</changeDirection>
			<wait>85</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>90</term>
			</changeDirection>
			<wait>90</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>95</term>
			</changeDirection>
			<wait>95</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>100</term>
			</changeDirection>
			<wait>100</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>105</term>
			</changeDirection>
			<wait>105</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>110</term>
			</changeDirection>
			<wait>110</wait>
			<changeDirection>
				<direction type="relative">20*$1</direction>
				<term>115</term>
			</changeDirection>
			<wait>115</wait>
		</action>
	</bullet>
	
	<action label="top">
		<wait>10+$rand*50</wait>
		<fire>
			<direction type="absolute">0</direction>
			<speed type="absolute">3.5</speed>
			<bulletRef label="spinz">
				<param>1</param>
			</bulletRef>
		</fire>
		<repeat>
			<times>19</times>
			<action>
				<fire>
					<direction type="sequence">360/20</direction>
					<speed type="sequence">0</speed>
					<bulletRef label="spinz">
						<param>1</param>
					</bulletRef>
				</fire>
			</action>
		</repeat>
		<fire>
			<direction type="sequence">5</direction>
			<speed type="absolute">5</speed>
			<bulletRef label="spinz">
				<param>1</param>
			</bulletRef>
		</fire>
		<repeat>
			<times>19</times>
			<action>
				<fire>
					<direction type="sequence">360/20</direction>
					<speed type="sequence">0</speed>
					<bulletRef label="spinz">
						<param>1</param>
					</bulletRef>
				</fire>
			</action>
		</repeat>
		<fire>
			<direction type="sequence">5</direction>
			<speed type="absolute">6.5</speed>
			<bulletRef label="spinz">
				<param>1</param>
			</bulletRef>
		</fire>
		<repeat>
			<times>19</times>
			<action>
				<fire>
					<direction type="sequence">360/20</direction>
					<speed type="sequence">0</speed>
					<bulletRef label="spinz">
						<param>1</param>
					</bulletRef>
				</fire>
			</action>
		</repeat>
		<vanish/>
	</action>

</bulletml>